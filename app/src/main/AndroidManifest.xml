<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shubham.odsadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ODSAdmin">
        <activity android:name=".activity.BookingInfoActivity"></activity>
        <activity android:name=".activity.BookingActivity" />
        <activity android:name=".activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.BillPendingActivity"
            android:label="Bills" />
        <activity
            android:name=".activity.BillInfoActivity"
            android:label="Bill Info" />
        <activity
            android:name=".activity.BillHistoryActivity"
            android:label="Bill History" />
        <activity
            android:name=".activity.UserInfoActivity"
            android:label="User Info" />
        <activity
            android:name=".activity.UserActivity"
            android:label="Users" />
        <activity
            android:name=".activity.WorkerInfoActivity"
            android:label="Worker Info" />
        <activity
            android:name=".activity.WorkerActivity"
            android:label="Workers" />
        <activity android:name=".activity.HomeActivity" />
    </application>

</manifest>